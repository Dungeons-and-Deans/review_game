
<div class="btn-group" id="group-choices">
  <% @game_session.groups.each do |group| %>
    <label class="btn btn-primary" id="<%= group.id %>">
      <input type="checkbox" value="<%= group.id %>" autocomplete="off"> <%= group.name %>
    </label>
  <% end %>
</div>

<button id="send-competition-question" class="mdl-button mdl-js-button mdl-button--raised mdl-button--colored">Send Competition Question</button>
