<% @groups.each do |group| %>
  <div class="group">
    <div><%= group.name %></div>
    <div><%= group.score %> points</div>
    <ol>
      <% group.supplies.each do |supply| %>
        <li><%= supply.amount %> <%= supply.name %></li>
      <% end %>
      <li><%= render partial: 'supply', locals: { id: group.id } %></li>
    </ol>
  </div>
<% end %>
