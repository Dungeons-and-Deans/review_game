<div class="mdl-card__title">
  <h3 class="mdl-card__title-text">Scores</h3>
</div>
<div class="mdl-card__supporting-text">
  <% @game_session.groups.each do |group| %>
    <div>
      <%= group.name %>:
      <span id="update_score<%= group.id %>">
        <%= link_to group.score, edit_score_path(@game_session, group), remote: true %>
      </span>
    </div>
  <% end %>
</div>
